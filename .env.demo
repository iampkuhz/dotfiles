#github token 用来提高git clone的访问限制
GITHUB_TOKEN='github_pat_***'

# OpenAI（必须）
OPENAI_API_KEY="sk-proj-***"

# 可选：想显式写就写，不写就吃 70-ai.zsh 默认值

## 使用 openai api 做行内补全样例
#AUTOCOMPLETE_API_BASE=https://api.openai.com/v1
#AUTOCOMPLETE_API_KEY=sk-***
#AUTOCOMPLETE_MODEL=gpt-5.1-codex-mini
## 行内补全：指向 Ollama 的样例
#AUTOCOMPLETE_API_BASE=http://127.0.0.1:11434/v1
#AUTOCOMPLETE_API_KEY=local-anything
#AUTOCOMPLETE_MODEL=

# Chat / Edit / Apply
CHAT_API_BASE=https://api.openai.com/v1
CHAT_API_KEY=sk-***
CHAT_MODEL=gpt-5-mini

# 百炼 Claude/Anthropic 兼容配置：在 ~/.env 中填写真实值
# 用百炼 API KEY 替换 YOUR_DASHSCOPE_API_KEY
ANTHROPIC_BASE_URL="https://dashscope.aliyuncs.com/apps/anthropic"
# API_KEY 和 AUTH_TOKEN 二选一，通常用 AUTH_TOKEN
# ANTHROPIC_API_KEY="YOUR_DASHSCOPE_API_KEY"
ANTHROPIC_AUTH_TOKEN="YOUR_DASHSCOPE_API_KEY"
ANTHROPIC_MODEL="qwen3-coder-plus"
# 可选：小模型/快模型（如果你希望 Claude Code 内部“快请求”走更便宜的）
ANTHROPIC_SMALL_FAST_MODEL="qwen3-coder-flash"

# 可选：你用 Continue 做行内补全时需要一个本地补全模型名（不写也吃默认）
# OLLAMA_AUTOCOMPLETE_MODEL="qwen2.5-coder-feipi:0.5b_boost2"
